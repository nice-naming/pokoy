{
  "name": "pokoy-app",
  "description": "A meditation app for impulsive minds",
  "version": "2.2.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test": "react-scripts test",
    "format": "react-scripts format",
    "build-functions": "cd functions && npm install && npm run build",
    "release": "standard-version",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@3m1/service-worker-updater": "^2.0.3",
    "@reduxjs/toolkit": "^1.8.1",
    "@saulpalv/useonline": "^1.0.1",
    "axios": "^0.21.1",
    "date-fns": "^2.25.0",
    "firebase": "^9.2.0",
    "logrocket": "^3.0.1",
    "nosleep.js": "^0.12.0",
    "rc-progress": "^3.2.2",
    "react": "^17.0.1",
    "react-charts": "^3.0.0-beta.34",
    "react-dom": "^17.0.1",
    "react-firebase-hooks": "^5.0.3",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.1",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-core": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "styled-components": "^5",
    "use-long-press": "^2.0.2",
    "use-sound": "^2.0.1",
    "uuid": "^8.3.2",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/lodash": "^4.14.167",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-swipeable-views": "^0.13.1",
    "@types/react-swipeable-views-core": "^0.13.0",
    "@types/react-swipeable-views-utils": "^0.13.3",
    "@types/styled-components": "^5.1.15",
    "@types/uuid": "^8.3.1",
    "eslint-config-code-complexity": "^1.1.1",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react-error-overlay": "6.0.9",
    "react-test-renderer": "^17.0.1",
    "source-map-explorer": "^2.5.2",
    "standard-version": "^9.3.2",
    "typescript": "^4.1.3",
    "typescript-styled-plugin": "^0.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
